#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0; i<n; i++)
    {
        cin>>arr[i];
 
    }
    int a;
    cin>>a;
    int i,cot=0;
    for(i=0; i<n; i++)
    {
        if(arr[i]==a)
        {
            cot++;
            break;
 
        }
    }
 
        if(cot==0)
        {
            i=-1;
 
        }
 
 
    cout<<i;
    return 0;
}
