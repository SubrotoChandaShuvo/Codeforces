#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int arr[n],i;
    for(i=0; i<n; i++)
    {
        cin>>arr[i];
    }
   
    for(i=0; i<n; i++)
     {
        if(arr[i]<=10)
        {
            cout<<"A["<<i<<"] = "<<arr[i]<<endl;
            

        }

     }


}
