#include<bits/stdc++.h>
using namespace std;
int main()
{
    string a,b,c,d;
    char temp;
    int lena,lenb;
    getline(cin, a);
    getline(cin, b);
    lena=a.size();
    lenb=b.size();
    c=a+b;
    temp=a[0];
    a[0]=b[0];
    b[0]=temp;
    cout<<lena<<" ";
    cout<<lenb<<endl;
    cout<<c<<endl;
    cout<<a<<" ";
    cout<<b;

}
